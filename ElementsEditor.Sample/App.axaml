<Application xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             x:Class="ElementsEditor.Sample.App"
			 xmlns:edior="clr-namespace:ElementsEditor;assembly=ElementsEditor"
             RequestedThemeVariant="Default">
             <!-- "Default" ThemeVariant follows system theme variant. "Dark" or "Light" are other available options. -->

	<Application.Resources>
		<ResourceDictionary>
			<ResourceDictionary.MergedDictionaries>
				<ResourceInclude Source="avares://ElementsEditor/Assets/ElementsEDitorResources.axaml"/>
				<ResourceInclude Source="/Assets/ElementsEditorSampleResources.axaml"/>
			</ResourceDictionary.MergedDictionaries>
		</ResourceDictionary>
	</Application.Resources>
    <Application.Styles>
        <FluentTheme />
		<StyleInclude Source="avares://ElementsEditor/Assets/ElementsEditorStyles.axaml"/>
		<StyleInclude Source="/Assets/ElementsEditorSampleStyles.axaml"/>
    </Application.Styles>
</Application>